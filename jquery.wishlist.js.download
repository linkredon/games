$(function(){});function clear_wishlist_table(){$("#wishlist_table tbody").empty();$("#wishlist_table tbody").html("<tr></tr>");}
function load_wishlist_table(){clear_wishlist_table();$.ajax({url:SITE_SALES_ROOT_PI+"/wishlist_dropdown_ajax.php",dataType:"jsonp",success:function(data){table_row=""
wishes=0;if(data.loggedin&&data.loggedin=="no"){table_row='<tr class="wishlist_row"><td colspan="2">You need to be logged in to make a wishlist</td></tr>';$('#wishlist_table tr:last').before(table_row);}else{for(var i=0;i<data.length;i++){table_row+='<tr class="wishlist_row"><td class="wishlist_name"><a href="'+SITE_SALES_ROOT_PI+'/wishlist.php">'+data[i].name+'</a></td>		<td class="wishlist_qty">'+data[i].qty+'</td></tr>';wishes+=Number(data[i].qty);}
if(wishes){$('#wishlist_table tr:last').before(table_row);}else{table_row='<tr class="wishlist_row"><td colspan="2"><a href="'+SITE_SALES_ROOT_PI+'/wishlist.php">Make a wishlist</a></td></tr>';$('#wishlist_table tr:last').before(table_row);}}}});}